{
  "name": "ogre",
  "version": "3.0.4",
  "description": "ogr2ogr web client",
  "keywords": [
    "ogr2ogr",
    "GIS",
    "GeoJSON"
  ],
  "author": "Marc Harter <wavded@gmail.com>",
  "repository": {
    "type": "git",
    "url": "http://github.com/wavded/ogre.git"
  },
  "homepage": "http://github.com/wavded/ogre",
  "bin": {
    "ogre": "dist/cli.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublishOnly": "rm -rf dist && tsc && cp -rf public dist",
    "build": "rm -rf dist && tsc && cp -rf public dist",
    "start": "ts-node cli.ts",
    "test": "nyc ts-node node_modules/.bin/blue-tape index_test.ts",
    "lint": "tsc --noEmit && eslint --ext ts .",
    "fmt": "prettier --write .",
    "fmt-check": "prettier --check ."
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "connect-multiparty": "^2.2.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-promise-router": "^4.1.1",
    "multer": "1.4.5-lts.1",
    "ogr2ogr": "^4.0.1"
  },
  "devDependencies": {
    "@types/blue-tape": "^0.1.33",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.14.6",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "blue-tape": "^1.0.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "nyc": "^15.1.0",
    "prettier": "^2.8.4",
    "prettier-plugin-organize-imports": "^3.2.2",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=12"
  }
}
